<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoubleWaveView">
        <!-- 水位高度 -->
        <attr name="dw_base_line" format="integer" />
        <!-- 浪高 -->
        <attr name="dw_wave_level" format="integer" />
        <!-- 背景颜色 -->
        <attr name="dw_bg_color" format="color" />
        <!-- 波浪颜色 -->
        <attr name="dw_wave_color" format="color" />
        <!-- 波浪速度 -->
        <attr name="dw_speed" format="float" />
        <!-- 波浪透明度 -->
        <attr name="dw_wave_alpha" format="integer" />
        <!-- 是否裁剪圆形 -->
        <attr name="dw_circle" format="boolean" />
    </declare-styleable>
</resources>